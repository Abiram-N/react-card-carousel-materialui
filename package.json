        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": false,
        "singleQuote": false,
        "trailingComma": "es5",
        "bracketSpacing": false,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid",
        "endOfLine": "lf"
    },
    "name": "react-card-carousel-materialui",
    "author": "Abiram Natarajan",
    "repository": {
        "type": "git",
        "url": "https://github.com/Abiram-N/react-card-carousel-materialui"
    },
    "module": "dist/react-card-carousel-materialui.esm.js",
    "size-limit": [
        {
            "path": "dist/react-card-carousel-materialui.cjs.production.min.js",
